class Solution {
public:
    int subtractProductAndSum(int n) {
        int prod=1,sum=0;
        int digit;
        while(n!=0){
            
            digit = n%10;
            prod = prod * digit;
            sum = sum + digit;
            n = n/10;
        }   
        int result= prod-sum;
        return result; 
    }
};
